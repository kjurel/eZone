{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@api/*": ["src/components/pages/api/*"],
      "@schemas/*": ["src/schemas/*"],
      "@utilities/*": ["src/utilities/*"]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
